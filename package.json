{
  "name": "@corollarium/youtubedl-wrapper",
  "version": "0.1.0",
  "description": "Youtube-dl wrapper for nodejs",
  "homepage": "https://github.com/Corollarium/youtubedl-wrapper",
  "repository": {
    "type": "git",
    "url": "https://github.com/Corollarium/youtubedl-wrapper"
  },
  "author": {
	  "name": "Corollarium",
	  "url": "https://corollarium.com"
	},
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "yarn test -- --coverage",
    "download": "node scripts/download.js ./bin/youtube-dl",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "lint": "eslint .",
    "docs": "documentation readme src --section=API",
    "postdocs": "git add README.md",
    "prebuild": "yarn run docs",
    "prerelease": "yarn run lint && yarn test",
    "release": "standard-version",
    "postpublish": "git push origin master --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "youtubedl",
    "video"
  ],
  "dependencies": {
    "got": "11.5.2"
  },
  "devDependencies": {
    "documentation": "11.0.1",
    "eslint": "6.0.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-prettier": "3.1.0",
    "husky": "2.4.1",
    "jest": "24.8.0",
    "lint-staged": "8.2.1",
    "open-cli": "5.0.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "standard-version": "6.0.1"
  }
}
